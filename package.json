  "dependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@strapi/plugin-cloud": "5.19.0",
    "@strapi/plugin-users-permissions": "5.19.0",
    "@strapi/strapi": "5.19.0",
    "pg": "8.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}
